{
  "name": "terraforms-full-metadata",
  "version": "1.0.0",
  "description": "Full metadata set of available Terraforms by Mathcastles.",
  "main": "index.js",
  "author": "uronsol <uronsolana@gmail.com>",
  "license": "MIT",
  "scripts": {
    "classes": "ts-node supplemental/createClasses.ts",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './contracts/types' './contracts/*.json'",
    "lint": "eslint --ext .ts,.tsx --fix",
    "normalize": "ts-node normalize.ts",
    "subgraph": "ts-node supplemental/querySubgraph.ts",
    "postinstall": "yarn compile-contract-types",
    "prettier": "prettier --write .",
    "start": "ts-node index.ts"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.1",
    "@ethersproject/units": "^5.5.0",
    "convert-csv-to-json": "^1.3.1",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "graphql": "16.2.0",
    "json2csv": "^5.0.6",
    "urql": "2.0.6"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^8.0.5",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typechain": "^6.1.0",
    "typescript": "^4.5.4"
  }
}
